{
    "name": "spatie/sun",
    "description": "Get information on the position of the sun",
    "keywords": [
        "spatie",
        "sun"
    ],
    "homepage": "https://github.com/spatie/sun",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "nesbot/carbon": "^3.8.3"
    },
    "require-dev": {
        "pestphp/pest": "^3.7.1",
        "spatie/test-time": "^1.3.3|^2.0",
        "symfony/var-dumper": "^4.3|^5.4.48"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Sun\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Sun\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
